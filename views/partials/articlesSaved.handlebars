<div data-_id="{{_id}}" class="card">
    <div class="card-header">
        <h3>
            <a class="article-link" target="_blank" rel="noopener noreferrer" href="{{link}}">{{title}}</a>
            <a class="btn btn-danger removeSaved">Delete From Saved</a>
            <a class="btn btn-info viewNotes">Article Notes</a>
        </h3>
    </div>
    <div class="card-body">
        {{body}}
    </div>
</div>